import pandas as pd
df = pd.read_csv('sample_dataset.csv')
df = df.dropna(axis=1,how='all')
df = df.dropna(axis=0,how='all')
print(df)

#1. Identifying the missing values using isnull() and notnull()
print("Missing values using isnull: ", df.isnull().sum())
print("Non Missing values using notnull: ", df.notnull().sum())
df['Age']=df['Age'].fillna(df['Age'].mean())
print(df)

#2. Apply custom functions 
def convert_satisfaction(x):
    return "High" if x>0.7 else "Low"
df["Satisfaction_Binary"] = df["Satisfaction_Level"].apply(convert_satisfaction)
print(df)

#3. map()
map_data = {"High":2,"Medium":1,"Low":0}
df["Purchase_history_altered"] = df["Purchase_History"].map(map_data)
print(df)


#4. Identifying outliers on income
from scipy import stats

#z-score
df["Income_Zscore"] = stats.zscore(df["Income"],nan_policy='omit')
z_outliers = df[df["Income_Zscore"].abs()>3]

#IQR Method
Q1 = df["Income"].quantile(0.25)
Q3 = df["Income"].quantile(0.75)
IQR = Q3 - Q1
lower_bound = Q1 - 1.5*IQR
upper_bound = Q3+ 1.5*IQR

iqr_outliers = df[(df["Income"] <lower_bound) || (df["Income"] > upper_bound)]
print(iqr_outliers)

